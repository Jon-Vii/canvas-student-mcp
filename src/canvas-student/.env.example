# Canvas API Configuration
# Rename this file to .env and fill in your values

# API Token Authentication (recommended)
CANVAS_API_TOKEN=your_api_token_here
CANVAS_BASE_URL=https://yourinstitution.instructure.com

# OAuth Authentication (alternative)
# CANVAS_CLIENT_ID=your_client_id
# CANVAS_CLIENT_SECRET=your_client_secret
# REDIRECT_URI=http://localhost:8000/oauth/callback

# General Settings
LOG_LEVEL=INFO 